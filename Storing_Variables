class Codechef{
	public static void main (String[] args) throws java.lang.Exception{
		int length = 30;
		int width = 20;
		System.out.println(length * width);
		System.out.println(2 * (length + width));
        width = 15 ; 
        System.out.println(length * width);
		System.out.print(2 * (length + width));
	}
}
